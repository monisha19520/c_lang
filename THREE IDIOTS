#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    float a,b,c,d;
    scanf("%f\n%f\n%f\n%f\n",&a,&b,&c,&d);
    printf("Binoy's house is located at (%.1f,%.1f)",(a+c)/2,(b+d)/2);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
