#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int a,num;
    scanf("%d\n",&num);
    if(num>0)
    printf("The Given Number %d is Positive.",num);
    else if(num<0)
    printf("The Given Number %d is Negative.",num);
    else
    {
        printf("The Given Number %d is Zero.",num);
    }
    
    

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
