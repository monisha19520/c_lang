#include<stdio.h>
#include<math.h>
#define PI 3.14159265358979323846
int main()
{
    float l,b,h,r;
    scanf("%f\n%f\n%f\n%f\n",&l,&b,&h,&r);
    float c=floor((PI*r*r*h)*100)/100;
    float o=floor((PI*r*r*h/3)*100)/100;
    float s=floor(((1.33333)*PI*r*r*r)*100)/100;
    printf("Volume of Cubiod=%.2f\nVolume of Cylinder=%.2f\nVolume of Cone=%.2f\nVolume of Shpere=%.2f",(l*b*h),c,o,s);
}
