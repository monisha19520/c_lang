#include <stdio.h>

int main() {
    int number;
    scanf("%d", &number);

    if (number < 0 || number > 9) {
        printf("Invalid Input\n");
        return 0;
    }

    // Array of strings for numbers 0 to 9
    char *words[] = {
        "Zero", "One", "Two", "Three", "Four",
        "Five", "Six", "Seven", "Eight", "Nine"
    };

    printf("Integer is %d and String is %s\n", number, words[number]);

    return 0;
}
