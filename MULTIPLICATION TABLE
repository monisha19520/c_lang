#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

    #include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    for (int i = 1; i <= n; i++) {
        printf("%d * %d = %d\n", n, i, n * i);
    }

    return 0;
}


    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
