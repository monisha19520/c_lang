#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int a;
    char b;
    scanf("%d\n%c\n",&a,&b);
    char ch=(int)a;
    printf("Integer of %d is %c\n",a,ch);
    int cha=(char)b;
    printf("Character of %c is %d",b,cha);

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
