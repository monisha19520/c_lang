#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int a,l,s=0;
    scanf("%d/n",&a);
    int n=a;
    while(a>0)
    {
        l=a%10;
        a=a/10;
        s+=l;
    }
    if(n%s==0)
        printf("Harshad Number");
    else
        printf("Not Harshad Number");

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
